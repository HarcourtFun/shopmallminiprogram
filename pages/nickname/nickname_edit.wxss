page{
    background-color:#F0F0F0;  
}
.header{
    width:100%;
    background-color:#ffa500;
}
.header_name{
    line-height:50px;
    display: inline-block;
    font-family: "微软雅黑";
    color:#fff;
    margin-left:40%;
}
.button{
    line-height:50px;
    display: inline-block;
    text-align: right;
    font-family: "微软雅黑";
    color:#fff;
    margin-left:40%;
    font-size:16px;
}
.section{
    width:100%;
    height:50px;
    background-color:#fff;
    margin:10px auto;
}
.section input{
    width:100%;
    height:50px;
    background-color:#fff;
    margin:10px auto;
    padding-left:20px;
}

.keep_button{
    width:70%;
    height:40px;
    margin:20px auto;
    line-height:40px;
}